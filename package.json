{
  "name": "sassy-inline-gradients",
  "version": "1.2.0",
  "description": "Sass extension to inline gradients as base64-encoded images",
  "keywords": [
    "base64",
    "css",
    "gradient",
    "gradients",
    "image",
    "inline",
    "sass",
    "scss"
  ],
  "bugs": "https://github.com/borodean/sassy-inline-gradients/issues",
  "license": "MIT",
  "author": "Vadym Borodin <borodean@gmail.com> (http://borodean.com)",
  "contributors": [],
  "files": [
    "dist/**"
  ],
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "repository": "borodean/sassy-inline-gradients",
  "scripts": {
    "benchmark": "ts-node scripts/benchmark",
    "build": "run-s build:clean build:compile",
    "build:clean": "rimraf dist",
    "build:compile": "tsc",
    "build-test-page": "ts-node scripts/build-test-page",
    "ci": "run-p -c lint \"test --coverage\" typecheck",
    "format": "run-s format:*",
    "format:eslint": "eslint --ext .ts --fix --max-warnings 0 .",
    "format:prettier": "prettier --write .",
    "lint": "run-s -c lint:*",
    "lint:eslint": "eslint --ext .ts --max-warnings 0 .",
    "lint:prettier": "prettier --check .",
    "prepare": "husky install",
    "test": "jest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "canvas": "^2.8.0",
    "imagemin": "^7.0.1",
    "imagemin-optipng": "^8.0.0",
    "puppeteer": "^11.0.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.16.0",
    "@types/imagemin": "^7.0.1",
    "@types/imagemin-optipng": "^5.2.1",
    "@types/jest": "^27.0.2",
    "@types/jest-image-snapshot": "^4.3.1",
    "@types/lodash": "^4.14.177",
    "@types/puppeteer": "^5.4.4",
    "@types/sass": "^1.43.0",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.2.4",
    "eslint-plugin-sort-destructure-keys": "^1.4.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-tsdoc": "^0.2.14",
    "eslint-plugin-typescript-sort-keys": "^2.0.0",
    "husky": "^6.0.0",
    "jest": "^27.3.1",
    "jest-image-snapshot": "^4.5.1",
    "lint-staged": "^11.2.6",
    "lodash": "^4.17.21",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "sass": "^1.43.4",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  },
  "peerDependencies": {
    "sass": "^1.0.0"
  },
  "engines": {
    "node": ">=10"
  }
}
